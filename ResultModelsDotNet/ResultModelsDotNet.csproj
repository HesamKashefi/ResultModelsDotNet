<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>net9.0;net8.0;</TargetFrameworks>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<Description>ResultModelsDotNet makes the result of your methods and APIs cleaner!</Description>
		<PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
		<PackageProjectUrl>https://github.com/HesamKashefi/ResultModelsDotNet</PackageProjectUrl>
		<RepositoryUrl>https://github.com/HesamKashefi/ResultModelsDotNet.git</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<PackageTags>asp-net-core wpf dotnet csharp result result-models</PackageTags>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<Version>1.0.2</Version>
	</PropertyGroup>

	<ItemGroup>
		<None Include="..\LICENSE.txt">
			<Pack>True</Pack>
			<PackagePath></PackagePath>
		</None>
		<None Include=".\README.md" Pack="true" PackagePath="\"/>
	</ItemGroup>
</Project>
